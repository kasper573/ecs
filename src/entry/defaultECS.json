      "components": [
        {
          "definitionId": "e0aca893-61d5-4329-b3af-b21e918c958f",
          "id": "99c4de53-dd7c-41b0-903b-34402e0502cc",
          "properties": {
            "action": {
              "code": "({entity}) => `Go to ${entity.name}`"
            },
            "effect": {
              "code": "({ entity, system }) => {\n  const sm = system.modules.find((item) => item.constructor.name === \"SceneManager\");\n  for (const sceneId in sm?.scenes ?? []) {\n    if (sm.scenes[sceneId].name === entity.name) {\n      sm.sceneId = sceneId;\n      return;\n    }\n  }\n  return `Could not find a scene by name \"${entity.name}\"`;\n}"
            }
          }
        }
      ]
    }
